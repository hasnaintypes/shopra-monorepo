# Logging configuration example

LOG_LEVEL=info
LOG_TO_CONSOLE=true
LOG_TO_FILE=true
LOG_TO_KAFKA=false
SERVICE_NAME=logging-lib
KAFKA_BROKERS=localhost:9092
KAFKA_LOG_TOPIC=logs
KAFKA_CLIENT_ID=logging-lib
LOG_FILE_COMPRESSION=false
LOG_FILE_RETENTION_DAYS=7
KAFKA_RETRY_ATTEMPTS=3
KAFKA_RETRY_DELAY_MS=1000
